{
  "autocomplete": [
    {
      "displayText": "gql-client url=\"<url>\"",
      "description": "Configures our graphql client.",
      "snippet": "gql-client url=\"${1:url}\"",
      "descriptionMoreURL": "https://github.com/marko-js/urql"
    }
  ],
  "@url": {
    "type": "string",
    "required": true,
    "autocomplete": [
      {
        "description": "A relative URL to an HTML resource"
      }
    ]
  },
  "@fetch": {
    "type": "function",
    "autocomplete": [
      {
        "description": "A custom fetch implementation"
      }
    ]
  },
  "@fetchOptions": {
    "type": "object",
    "autocomplete": [
      {
        "description": "Additional fetch options that fetch should use to make a request"
      }
    ]
  },
  "@requestPolicy": {
    "enum": ["cache-first", "cache-and-network", "network-only", "cache-only"],
    "autocomplete": [
      {
        "description": "Default cache policy for requests."
      }
    ]
  }
}
